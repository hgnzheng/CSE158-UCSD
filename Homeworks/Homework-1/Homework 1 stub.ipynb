       "  'review/taste': 3.5,\n",
       "  'beer/name': 'Ashland Amber',\n",
       "  'review/timeUnix': 1248142254,\n",
       "  'beer/ABV': 5.4,\n",
       "  'beer/beerId': '10785',\n",
