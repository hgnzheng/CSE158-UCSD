       "  'user/gender': 'Male',\n",
       "  'beer/ABV': 3.5,\n",
       "  'beer/beerId': '436',\n",
       "  'beer/brewerId': '163',\n",
       "  'review/timeStruct': {'isdst': 0,\n",
       "   'mday': 6,\n",
