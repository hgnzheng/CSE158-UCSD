       "  'beer/ABV': 6.1,\n",
       "  'beer/beerId': '10784',\n",
       "  'beer/brewerId': '1075',\n",
       "  'review/timeStruct': {'isdst': 0,\n",
       "   'mday': 16,\n",
       "   'hour': 1,\n",
       "   'min': 19,\n",
