       "  'beer/ABV': 8.8,\n",
       "  'beer/beerId': '20818',\n",
       "  'beer/brewerId': '1075',\n",
       "  'review/timeStruct': {'isdst': 0,\n",
       "   'mday': 7,\n",
       "   'hour': 3,\n",
       "   'min': 15,\n",
