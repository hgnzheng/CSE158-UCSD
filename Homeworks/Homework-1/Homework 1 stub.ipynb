       "  'beer/ABV': 6.8,\n",
       "  'beer/beerId': '56071',\n",
       "  'beer/brewerId': '1075',\n",
       "  'review/timeStruct': {'isdst': 0,\n",
       "   'mday': 29,\n",
       "   'hour': 1,\n",
       "   'min': 20,\n",
